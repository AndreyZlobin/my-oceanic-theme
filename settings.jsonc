{
  "workbench.editorAssociations": {
    "*.tgz": "default"
  },
  "files.eol": "\n",
  "extensions.ignoreRecommendations": true,
  "explorer.confirmPasteNative": false,
  "explorer.confirmDelete": false,
  "git.confirmSync": false,

  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.gpuAcceleration": "on",

  "editor.unicodeHighlight.nonBasicASCII": false,
  "workbench.iconTheme": "material-icon-theme",

  // Semantic highlighting
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "rules": {

        // Типы и интерфейсы - зелёный italic
        "type": { "foreground": "#C3E88D", "fontStyle": "italic" },
        "interface": { "foreground": "#C3E88D", "fontStyle": "italic" },

        // Классы - золотой
        "class": "#FFCB6B",

        // Функции - золотой, локальные (стрелочные) - синий
        "function": "#FFCB6B",
        "function.defaultLibrary": "#FFCB6B",
        "function.local": { "foreground": "#82AAFF" },

        // Переменные - светлый, readonly/defaultLibrary - белый
        "variable": "#EEFFE3",
        "variable.readonly": "#EEFFFF",
        "variable.readonly.defaultLibrary": "#EEFFFF",
        "variable.defaultLibrary": "#EEFFFF",

        // Остальное
        "parameter": "#F78C6C",
        "property": "#EEFFFF",
        // property.declaration убран - VS Code не различает getter/setter от shorthand props
        "method": "#82AAFF",
        "typeParameter": "#FFCB6B",
        "namespace": "#C3E88D",
        "enum": "#FFCB6B",
        "enumMember": "#F78C6C",

    }
  },

  // ============================================================================
  // ШРИФТЫ
  // ============================================================================
  "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.fontWeight": "300",
  "editor.lineHeight": 24,
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "300",

  // ============================================================================
  // РЕДАКТОР
  // ============================================================================
  // Лучший поиск
  "search.showLineNumbers": true,
  // Sticky scroll - видишь в какой функции/классе находишься
  "editor.stickyScroll.enabled": true,
    // Компактнее проводник
  "workbench.tree.indent": 16,
  "workbench.activityBar.location": "top",
  "editor.quickSuggestions": {
    "strings": "on"
  },

  // Открывать файл только по двойному клику (как в WebStorm)
  "workbench.list.openMode": "doubleClick",
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "visible",
  "editor.scrollbar.horizontal": "visible",
  "editor.renderWhitespace": "selection",
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "smooth",
  "editor.guides.indentation": true,
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.occurrencesHighlight": "multiFile",
  "editor.selectionHighlight": true,
  "editor.rulers": [100, 120],
  "editor.accessibilitySupport": "off",

  // Ctrl + колесо мыши = zoom шрифта
  "editor.mouseWheelZoom": true,

  // Linked editing - переименование парных HTML/JSX тегов автоматически
  "editor.linkedEditing": true,

  // Подсветка парных скобок при наведении
  "editor.matchBrackets": "always",

  // Fold imports по умолчанию (можно раскрыть)
  "editor.foldingImportsByDefault": true,

  // Подсветка модифицированных табов
  "workbench.editor.highlightModifiedTabs": true,

  // Drag & drop текста
  "editor.dragAndDrop": true,

  // Умный выбор при двойном клике (не выделяет точки, дефисы)
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",

  // Показывать невидимые символы при выделении
  "editor.renderControlCharacters": true,

  // Hover - задержка перед показом подсказки (ms)
  "editor.hover.delay": 300,

  // Suggest - автодополнение
  "editor.suggest.preview": true,
  "editor.suggest.showStatusBar": true,
  "editor.acceptSuggestionOnEnter": "smart",

  // Breadcrumbs - навигация сверху
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.symbolPath": "on",

  // Восстанавливать все окна при открытии
  "window.restoreWindows": "all",

  // Не показывать Welcome tab
  "workbench.startupEditor": "none",

  // Показывать полный путь в title
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}$",

  // Explorer - сортировка файлов
  "explorer.sortOrder": "type",

  // // Скрыть мусор из поиска и explorer
  // "files.exclude": {
  //   "**/.git": true,
  //   "**/.DS_Store": true,
  //   "**/node_modules": true,
  //   "**/dist": true,
  //   "**/.nuxt": true,
  //   "**/.output": true
  // },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/pnpm-lock.yaml": true,
    "**/package-lock.json": true
  },

  // Плавный скролл
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,

  // Копировать всю строку если ничего не выделено
  "editor.emptySelectionClipboard": true,

  // Font ligatures (=> становится стрелкой, != становится ≠) - если шрифт поддерживает
  "editor.fontLigatures": true,

  // Показывать unused code приглушённым
  "editor.showUnused": true,

  // Inline suggestions (для Copilot и подобных)
  "editor.inlineSuggest.enabled": true,
  // ============================================================================
  // TYPESCRIPT
  // ============================================================================

  // Inlay Hints (подсказки прямо в коде)
  "editor.inlayHints.enabled": "on",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
  "typescript.inlayHints.enumMemberValues.enabled": true,

  // Автоимпорты
  "typescript.suggest.autoImports": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.suggest.autoImports": true,

  // Фиксы при сохранении (только ESLint, без встроенной сортировки - она может конфликтовать)
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },

  // Автоматическое обновление импортов при перемещении файлов
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",

  // Быстрые подсказки
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.suggest.includeCompletionsForImportStatements": true,

  // Проверка JS файлов как TS (если нет tsconfig)
  "js/ts.implicitProjectConfig.checkJs": true,

  // Использовать workspace версию TypeScript
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,

  // ============================================================================
  // ФАЙЛЫ И ФОРМАТИРОВАНИЕ
  // ============================================================================
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,

  // Prettier
  "prettier.requireConfig": true,

  // ESLint
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "vue"],

  // Stylelint
  "stylelint.enable": true,
  "stylelint.packageManager": "pnpm",
  "stylelint.configFile": "stylelint.config.mjs",
  "stylelint.validate": ["css", "scss", "vue", "html"],

  // Форматтеры по языкам
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[vue]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },

  // ============================================================================
  // GIT
  // ============================================================================
  "scm.defaultViewMode": "tree",
  "scm.diffDecorations": "all",
  "git.decorations.enabled": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",

  // ============================================================================
  // EXPLORER & TERMINAL
  // ============================================================================
  "explorer.openEditors.visible": 1,
  "explorer.compactFolders": false,

  // Линии отступов в дереве (как в WebStorm)
  "workbench.tree.renderIndentGuides": "always",

  // Git Bash как терминал - поддерживает Unix команды (rm, cp, cat, grep)
  "terminal.integrated.defaultProfile.windows": "Git Bash",

  // ============================================================================
  // ЦВЕТА ТЕМЫ (Material Oceanic)
  // ============================================================================
