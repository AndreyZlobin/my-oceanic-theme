  // ============================================================================
  // TEXTMATE RULES (для токенов без semantic highlighting)
  // ============================================================================
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      // Vue <script>, <style>, <template> tags - зелёный
      {
        "scope": ["entity.name.tag.script.html.vue", "entity.name.tag.style.html.vue", "entity.name.tag.template.html.vue"],
        "settings": { "foreground": "#26bda4" }
      },
      // Nested <template> inside main template - красный
      {
        "scope": ["meta.template-tag.start entity.name.tag.template.html.vue"],
        "settings": { "foreground": "#F07178" }
      },
      // Vue components (PascalCase tags like <ActiveBalance>) - зелёный
      {
        "scope": ["meta.tag.other.unrecognized.html.derivative entity.name.tag.html"],
        "settings": { "foreground": "#26bda4" }
      },
      // Tag brackets <> - голубой
      {
        "scope": ["punctuation.definition.tag.begin.html.vue", "punctuation.definition.tag.end.html.vue"],
        "settings": { "foreground": "#89DDFF" }
      },
      // Vue directives (v-if, v-for, v-show, etc) - золотой
      {
        "scope": ["keyword.control.conditional.vue", "keyword.control.loop.vue", "meta.attribute.directive"],
        "settings": { "foreground": "#FFCB6B", "fontStyle": "italic" }
      },
      // CSS element selectors (body, div, span, etc) - красный
      {
        "scope": ["entity.name.tag.css"],
        "settings": { "foreground": "#F07178" }
      },
      // CSS pseudo-classes (:root, :hover, etc) - фиолетовый
      {
        "scope": ["entity.other.attribute-name.pseudo-class.css", "entity.other.attribute-name.pseudo-element.css"],
        "settings": { "foreground": "#C792EA", "fontStyle": "italic" }
      },
      // CSS variables (--text-muted, etc) - золотой
      {
        "scope": ["variable.css", "variable.argument.css"],
        "settings": { "foreground": "#FFCB6B" }
      },
      // CSS property values (transparent, inherit, fonts, etc) - красный
      {
        "scope": ["support.constant.property-value.css", "support.constant.font-name.css", "support.constant.color.w3c-standard-color-name.css", "meta.property-value.css"],
        "settings": { "foreground": "#F07178" }
      },
      // CSS functions (var, calc, rgb, etc) - синий
      {
        "scope": ["support.function.misc.css", "support.function.transform.css", "support.function.calc.css", "support.function.gradient.css"],
        "settings": { "foreground": "#82AAFF" }
      },
      // CSS vendor prefixes (-apple-system, etc) and units (px, em, %) - красный
      {
        "scope": ["support.constant.vendored.property-value.css", "keyword.other.unit.css", "keyword.other.unit.px.css", "keyword.other.unit.rem.css", "keyword.other.unit.em.css", "keyword.other.unit.deg.css", "keyword.other.unit.s.css", "keyword.other.unit.ms.css", "keyword.other.unit.vh.css", "keyword.other.unit.vw.css"],
        "settings": { "foreground": "#F07178", "fontStyle": "" }
      },
      // CSS colors (#ff6b6b, etc) - голубой
      {
        "scope": ["constant.other.color.rgb-value.hex.css", "constant.other.color.rgb-value.css", "keyword.other.unit.percentage.css"],
        "settings": { "foreground": "#89DDFF" }
      },
      // CSS numbers (48, 0.5, etc) - красный
      {
        "scope": ["constant.numeric.css"],
        "settings": { "foreground": "#F07178" }
      },
      // CSS @keyframes - фиолетовый
      {
        "scope": ["keyword.control.at-rule.keyframes.css"],
        "settings": { "foreground": "#C792EA", "fontStyle": "italic" }
      },
      // CSS keyframe name (spin) and offsets (from, to) - золотой
      {
        "scope": ["variable.parameter.keyframe-list.css", "entity.other.keyframe-offset.css", "entity.other.keyframe-offset.percentage.css"],
        "settings": { "foreground": "#FFCB6B" }
      },
      // All imports - белый (различить default/named невозможно в textMate)
      {
        "scope": ["variable.other.readwrite.alias"],
        "settings": { "foreground": "#EEFFE3" }
      },
      {
        "scope": ["variable.other.readwrite"],
        "settings": { "foreground": "#EEFFE3" }
      },

      // Keywords (const, let, var, class, function, new, etc)
      {
        "scope": ["keyword", "keyword.operator.new", "storage.type", "storage.modifier"],
        "settings": { "foreground": "#C792EA", "fontStyle": "italic" }
      },
      // Import/Export/From (без italic)
      {
        "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from", "keyword.control.default", "keyword.operator.expression.import"],
        "settings": { "foreground": "#C792EA" }
      },
      // Import * - голубой
      {
        "scope": ["constant.language.import-export-all"],
        "settings": { "foreground": "#89DDFF" }
      },
      // Template literal ${} - голубой
      {
        "scope": ["punctuation.definition.template-expression.begin", "punctuation.definition.template-expression.end"],
        "settings": { "foreground": "#89DDFF" }
      },
      // Control flow (if/else/return/try/catch)
      {
        "scope": ["keyword.control.conditional", "keyword.control.trycatch", "keyword.control.flow"],
        "settings": { "foreground": "#C792EA" }
      },
      // as, keyof, typeof, infer - фиолетовый
      {
        "scope": ["keyword.control.as", "keyword.operator.expression.keyof", "keyword.operator.expression.typeof", "keyword.operator.expression.infer"],
        "settings": { "foreground": "#C792EA" }
      },
      // Type operators (|, &)
      {
        "scope": ["keyword.operator.type"],
        "settings": { "foreground": "#89DDFF" }
      },
      // Operators (=, ==, +, -, =>, etc)
      {
        "scope": ["keyword.operator", "storage.type.function.arrow"],
        "settings": { "foreground": "#89DDFF", "fontStyle": "" }
      },
      // Punctuation
      {
        "scope": ["punctuation.accessor", "punctuation.separator", "punctuation.terminator", "punctuation.definition.block", "punctuation.definition.parameters", "punctuation.definition.array", "punctuation.definition.typeparameters", "meta.brace"],
        "settings": { "foreground": "#89DDFF" }
      },
      // Comments
      {
        "scope": ["comment", "comment.line", "comment.block", "punctuation.definition.comment"],
        "settings": { "foreground": "#546E7A", "fontStyle": "italic" }
      },
      // JSDoc tags
      {
        "scope": ["storage.type.class.jsdoc", "entity.name.type.instance.jsdoc"],
        "settings": { "foreground": "#C792EA", "fontStyle": "bold italic" }
      },
      // Strings
      {
        "scope": ["string", "string.quoted", "string.regexp"],
        "settings": { "foreground": "#C3E88D" }
      },
      // String escapes
      {
        "scope": ["constant.character.escape", "constant.other.placeholder"],
        "settings": { "foreground": "#89DDFF" }
      },
      // Numbers
      {
        "scope": ["constant.numeric"],
        "settings": { "foreground": "#F78C6C" }
      },
      // Boolean, null, undefined
      {
        "scope": ["constant.language.boolean", "constant.language.null", "constant.language.undefined", "constant.language.nan"],
        "settings": { "foreground": "#C792EA", "fontStyle": "italic" }
      },
      // this, super
      {
        "scope": ["variable.language.this", "variable.language.super"],
        "settings": { "foreground": "#C792EA" }
      },
      // Primitive types (string, number, boolean)
      {
        "scope": ["support.type.primitive", "support.type.builtin"],
        "settings": { "foreground": "#C792EA" }
      },
      // Type parameters (T, K, V)
      {
        "scope": ["entity.name.type.parameter"],
        "settings": { "foreground": "#FFCB6B" }
      },
      // Console
      {
        "scope": ["support.class.console"],
        "settings": { "foreground": "#FFCB6B" }
      },
      // Debugger
      {
        "scope": ["keyword.other.debugger"],
        "settings": { "foreground": "#F78C6C" }
      },
      // HTML/JSX tags
      {
        "scope": ["entity.name.tag", "punctuation.definition.tag"],
        "settings": { "foreground": "#F07178" }
      },
      // HTML/JSX attributes
      {
        "scope": ["entity.other.attribute-name"],
        "settings": { "foreground": "#FFCB6B", "fontStyle": "italic" }
      },
      // Decorators
      {
        "scope": ["meta.decorator", "punctuation.decorator"],
        "settings": { "foreground": "#82AAFF" }
      },
      // JSON keys
      {
        "scope": ["support.type.property-name.json", "meta.structure.dictionary.key.json"],
        "settings": { "foreground": "#C792EA" }
      },
      // JSON constants
      {
        "scope": ["constant.language.json"],
        "settings": { "foreground": "#F78C6C", "fontStyle": "italic" }
      },
      // CSS property names
      {
        "scope": ["support.type.property-name.css"],
        "settings": { "foreground": "#B2CCD6" }
      },
      // CSS classes/IDs
      {
        "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
        "settings": { "foreground": "#FFCB6B" }
      },
      // Function calls - italic
      {
        "scope": ["meta.function-call entity.name.function"],
        "settings": { "foreground": "#82AAFF", "fontStyle": "italic" }
      },
      // Getter/Setter declarations - синий
      {
        "scope": ["meta.definition.method entity.name.function"],
        "settings": { "foreground": "#82AAFF" }
      },
      // Object variables (Sentry, axios, etc) - белый
      {
        "scope": ["variable.other.object"],
        "settings": { "foreground": "#EEFFFF" }
      },
      // Properties without type info (any) - grey fallback
      {
        "scope": ["variable.other.property"],
        "settings": { "foreground": "#b7bbbd" }
      },
    ]
  },

  // ============================================================================
  // MISC
  // ============================================================================
  "claudeCode.preferredLocation": "panel",
  "security.workspace.trust.untrustedFiles": "open",
  "git.autofetch": true,
  "workbench.colorTheme": "My Oceanic"
}
